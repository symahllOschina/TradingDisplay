<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    >
    <lecho.lib.hellocharts.view.PieChartView
        android:id="@+id/tvmain_pieChartView"
        android:layout_width="wrap_content"
        android:layout_height="200dp"
        android:layout_weight="1"
        >
    </lecho.lib.hellocharts.view.PieChartView>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:layout_marginRight="50dp"
        >
        <LinearLayout
            android:id="@+id/tvmain_piechartview_tvWX"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal">
            <View
                android:layout_width="25dp"
                android:layout_height="15dp"
                android:background="@color/green_30d60a"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="微信"
                android:textSize="@dimen/text_size14"
                android:textColor="@color/white_f2f2f2"
                />
            <TextView
                android:id="@+id/tvmain_piechartview_tvWXText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/text_size14"
                android:textColor="@color/white_f2f2f2"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/tvmain_piechartview_tvALI"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:gravity="center"
            android:orientation="horizontal">
            <View
                android:layout_width="25dp"
                android:layout_height="15dp"
                android:background="@color/blue_38AFFF"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="支付宝"
                android:textSize="@dimen/text_size14"
                android:textColor="@color/white_f2f2f2"
                />
            <TextView
                android:id="@+id/tvmain_piechartview_tvAliText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/text_size14"
                android:textColor="@color/white_f2f2f2"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/tvmain_piechartview_tvYZF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:gravity="center"
            android:orientation="horizontal">
            <View
                android:layout_width="25dp"
                android:layout_height="15dp"
                android:background="@color/red_FF1200"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="翼支付"
                android:textSize="@dimen/text_size14"
                android:textColor="@color/white_f2f2f2"
                />
            <TextView
                android:id="@+id/tvmain_piechartview_tvYZFText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/text_size14"
                android:textColor="@color/white_f2f2f2"
                />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/tvmain_piechartview_tvBANK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:gravity="center"
            android:orientation="horizontal">
            <View
                android:layout_width="25dp"
                android:layout_height="15dp"
                android:background="@color/black_333"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="银行卡"
                android:textSize="@dimen/text_size14"
                android:textColor="@color/white_f2f2f2"
                />
            <TextView
                android:id="@+id/tvmain_piechartview_tvBANKText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/text_size14"
                android:textColor="@color/white_f2f2f2"
                />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/tvmain_piechartview_tvYL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:gravity="center"
            android:orientation="horizontal">
            <View
                android:layout_width="25dp"
                android:layout_height="15dp"
                android:background="@color/red_FF29A9"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="银联二维码"
                android:textSize="@dimen/text_size14"
                android:textColor="@color/white_f2f2f2"
                />
            <TextView
                android:id="@+id/tvmain_piechartview_tvYLText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/text_size14"
                android:textColor="@color/white_f2f2f2"
                />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>
